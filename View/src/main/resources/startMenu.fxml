<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" prefHeight="350.0" prefWidth="480.0" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sudokupackage.StartMenu">
   <children>
      <AnchorPane fx:id="combobox" prefHeight="350.0" prefWidth="480.0">
         <children>
            <ComboBox fx:id="comboBoxLang" layoutX="130.0" layoutY="112.0" prefHeight="26.0" prefWidth="221.0" promptText="%comboLang" />
            <ComboBox fx:id="comboBoxLevel" layoutX="130.0" layoutY="162.0" prefHeight="26.0" prefWidth="221.0" promptText="%comboLevel" />
            <Button layoutX="153.0" layoutY="213.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="63.0" prefWidth="174.0" text="Start Game">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Button>
            <Label layoutX="80.0" layoutY="47.0" text="%title">
               <font>
                  <Font name="System Bold" size="40.0" />
               </font>
            </Label>
            <Button fx:id="setLang" layoutX="368.0" layoutY="112.0" mnemonicParsing="false" onAction="#handleButtonSetLangAction" text="%setLangButton" />
         </children>
      </AnchorPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
